{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      // "config/*": ["config/*"],
      "constants/*": ["constants/*"],
      "mocks/*": ["mocks/*"],
      // "hooks/*": ["hooks/*"],
      "store/*": ["store/*"],
      // "services/*": ["services/*"],
      // "shared/*": ["shared/*"],
      // "styles/*": ["styles/*"],
      // "utils/*": ["utils/*"],
      "routes/*": ["routes/*"],
      "interfaces/*": ["interfaces/*"],
   },
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "vite-env.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}